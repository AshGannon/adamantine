geometry
{
  dim 2 ; dimension of the domain
  length 10
  height 20 ; In 3D, the third parameters is width
  length_divisions 10 ; Number of cell layers in the length direction
  height_divisions 20 ; Number of cell layers in the height direction
}

materials
{
  n_materials 1
  
  material_0
  {
    solid 
    {
      thermal_conductivity 10. ; The value can be constant or an equation
                               ; 10*exp(T)*sin(2.*T)
    }
    powder
    {
      thermal_conductivity 5.
    }
    liquid
    {
      thermal_conductivity 7.5 ; Not all three states need to define the same
                               ; properties or to exist
    }
  }
}
